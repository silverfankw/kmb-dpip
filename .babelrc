{
    "plugins": [
        [
            "babel-plugin-module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "@api": "./src/api",
                    "@assets": "./src/assets",
                    "@components": "./src/components",
                    "@styles": "./src/assets/styles",
                    "@hooks": "./src/hooks",
                    "@contexts": "./src/contexts",
                    "@reducers": "./src/reducers",
                    "@store": "./src/store",
                    "@theme": "./src/theme/theme",
                    "@utils": "./src/utils"
                }
            }
        ]
    ]
}